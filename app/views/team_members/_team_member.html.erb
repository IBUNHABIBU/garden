<div id="<%= dom_id team_member %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= team_member.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= team_member.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Bio:</strong>
    <%= team_member.bio %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Display order:</strong>
    <%= team_member.display_order %>
  </div>
</div>

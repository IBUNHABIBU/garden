<div id="<%= dom_id trekking %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= trekking.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= trekking.duration %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Difficulty:</strong>
    <%= trekking.difficulty %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Overview title:</strong>
    <%= trekking.overview_title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Overview description:</strong>
    <%= trekking.overview_description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Content:</strong>
    <%= trekking.content %>
